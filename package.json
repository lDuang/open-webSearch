{
  "name": "open-websearch",
  "version": "1.2.0",
  "description": "web search the internet",
  "type": "module",
  "bin": {
    "open-websearch": "./build/index.cjs"
  },
  "files": [
    "build"
  ],
  "scripts": {
    "build": "esbuild src/index.ts --bundle --platform=node --format=cjs --outfile=build/index.cjs",
    "watch": "tsc --watch",
    "inspector": "npx @modelcontextprotocol/inspector dist/index.js",
    "start": "npm run build && node build/index.cjs",
    "dev": "tsc && node dist/index.js",
    "test:stdio": "npm run build && set MODE=stdio && node build/index.cjs",
    "test:http": "npm run build && set MODE=http && node build/index.cjs",
    "test:both": "npm run build && set MODE=both && node build/index.cjs"
  },
  "dependencies": {
    "@modelcontextprotocol/sdk": "^1.11.2",
    "@types/axios": "^0.14.4",
    "@types/cheerio": "^0.22.35",
    "axios": "^1.7.9",
    "cheerio": "^1.0.0",
    "cors": "^2.8.5",
    "express": "^4.18.2",
    "https-proxy-agent": "^7.0.6",
    "npx": "^10.2.2"
  },
  "devDependencies": {
    "@types/cors": "^2.8.19",
    "@types/express": "^4.17.21",
    "@types/node": "^20.17.10",
    "esbuild": "^0.27.2",
    "typescript": "^5.3.3"
  }
}
